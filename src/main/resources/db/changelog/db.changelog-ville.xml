<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

    <changeSet id="3" author="dev">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="ville"/>
            </not>
        </preConditions>
        <createTable tableName="ville">
            <column name="insee_ville" type="NUMBER">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="code_postal" type="NUMBER">
                <constraints nullable="false"/>
            </column>
            <column name="nom" type="VARCHAR2(50)">
                <constraints nullable="false"/>
            </column>
            <column name="y_latitude" type="DOUBLE PRECISION"/>
            <column name="x_longitude" type="DOUBLE PRECISION"/>
            <column name="nom_departement" type="VARCHAR2(50)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
